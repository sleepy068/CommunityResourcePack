			<GameDBLocalizedString>	<LocID>SYM_CRP_MENORRAGIE</LocID>						<Text>月经过多</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>SYM_CRP_MENORRAGIE_DESCRIPTION</LocID>			<Text>月经流量异常大。</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>SYM_CRP_LEUCORRHEE</LocID>						<Text>白带</Text>	</GameDBLocalizedString>
