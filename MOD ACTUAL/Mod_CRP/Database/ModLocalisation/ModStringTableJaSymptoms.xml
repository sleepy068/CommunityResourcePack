			<GameDBLocalizedString>
				<LocID>SYM_CRP_ABOLITION_POULS</LocID>
				<Text>脈拍の途絶</Text>
			</GameDBLocalizedString>
			<GameDBLocalizedString>
				<LocID>SYM_CRP_ABOLITION_POULS_DESCRIPTION</LocID>
