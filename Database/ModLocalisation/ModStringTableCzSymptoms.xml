			<GameDBLocalizedString>	<LocID>SYM_CRP_ODEUR_VAGINALE</LocID>					<Text>Vaginální zápach</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>SYM_CRP_ODEUR_VAGINALE_DESCRIPTION</LocID>		<Text>Silný "rybí" zápach vagíny.</Text>	</GameDBLocalizedString>
