			<GameDBLocalizedString>	<LocID>SYM_CRP_OBGYN_VEGETATION_KYSTE</LocID>					<Text>Extracystická vegetace</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>SYM_CRP_OBGYN_VEGETATION_KYSTE_DESCRIPTION</LocID>		<Text>Vegetace zobrazená na ultrazvukovém snímku.</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>SYM_CRP_TUMEFACTION</LocID>						<Text>Otok prsou</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>SYM_CRP_TUMEFACTION_DESCRIPTION</LocID>			<Text>Abnormálně zvětšení prsou.</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>SYM_CRP_MASSE_HOMOGENE</LocID>					<Text>Homogenní hmota</Text>	</GameDBLocalizedString>
